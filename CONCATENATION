public class ConcatenateStrings {
    public static void main(String[] args) {
        // Given strings
        String str1 = "Hello, ";
        String str2 = "World!";

        // Concatenate the two strings
        String concatenatedString = str1 + str2;

        // Print the result
        System.out.println("Concatenated String: " + concatenatedString);
    }
}
