class R192221109 {
    private double length;
    private double width;

    // Constructor to initialize the length and width of the rectangle
    public R192221109(double length, double width) {
        this.length = length;
        this.width = width;
    }

    // Method to calculate and return the area of the rectangle
    public double calculateArea() {
        return length * width;
    }

    public static void main(String[] args) {
        // Creating a Rectangle object with length and width
        R192221109 rectangle = new R192221109(10.5, 7.2);

        // Calculating and displaying the area of the rectangle
        System.out.println("Area of the rectangle: " + rectangle.calculateArea());
    }
}
