class R192221109 {
    private String name;
    private int age;
    public R192221109(String name, int age) {
        this.name = name;
        this.age = age;
    }
    public void printPersonDetails() {
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
    }
    public static void main(String[] args) {
        R192221109 person = new R192221109("Alice", 30);
        person.printPersonDetails();
    }
}
